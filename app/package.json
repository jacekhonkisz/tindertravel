{
  "name": "app",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "npx expo start --dev-client",
    "start:fresh": "npx expo start --dev-client --clear",
    "start:ios": "npx expo start --dev-client --ios",
    "start:ios:fresh": "npx expo start --dev-client --clear --ios",
    "start:tunnel": "npx expo start --dev-client --tunnel --ios",
    "ios": "npx expo run:ios",
    "ios:release": "npx expo run:ios --configuration Release",
    "ios:device": "npx expo run:ios --device",
    "prebuild": "npx expo prebuild --clean --platform ios",
    "clean": "rm -rf node_modules .expo ios/build",
    "android": "expo run:android"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/core": "^7.12.4",
    "@react-navigation/elements": "^2.6.4",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/native-stack": "^7.3.26",
    "@react-navigation/stack": "^7.4.8",
    "expo": "~54.0.12",
    "expo-blur": "~15.0.7",
    "expo-dev-client": "~6.0.13",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.9",
    "expo-linear-gradient": "^15.0.7",
    "expo-maps": "^0.12.8",
    "expo-screen-orientation": "^9.0.7",
    "expo-status-bar": "~3.0.8",
    "react": "19.1.0",
    "react-native": "0.81.4",
    "react-native-deck-swiper": "^2.0.19",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-ios-kit": "^0.0.22",
    "react-native-maps": "1.20.1",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "^4.16.0",
    "react-native-super-grid": "^6.0.2",
    "react-native-svg": "15.12.1",
    "react-native-worklets": "^0.5.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@types/react": "~19.1.0",
    "@types/react-native": "^0.72.8",
    "babel-preset-expo": "^54.0.3",
    "typescript": "~5.9.2"
  },
  "private": true
